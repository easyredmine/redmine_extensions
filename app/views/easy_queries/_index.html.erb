<%
  query ||= @query
  entities ||= @entities
  formatter ||= @query.formatter
%>

<% present(query, formatter: formatter) do |presenter| %>
  <%= render 'easy_queries/settings', query: query %>

  <% presenter.outputs.each do |output| %>
    <%= output.render_data %>
  <% end %>
<% end %>

